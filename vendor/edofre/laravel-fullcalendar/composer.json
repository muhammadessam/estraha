{
  "name": "edofre/laravel-fullcalendar",
  "description": "Laravel component for fullcalendar package",
  "keywords": [
    "fullcalendar",
    "javascript",
    "event",
    "calendar",
    "laravel"
  ],
  "homepage": "https://github.com/edofre/laravel-fullcalendar",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.*",
    "npm-asset/fullcalendar": "v3.8.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.2.0|~3.3.0",
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Edofre\\Fullcalendar\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Edofre\\Fullcalendar\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Edofre\\Fullcalendar\\FullcalendarServiceProvider"
      ],
      "aliases": {
        "Fullcalendar": "Edofre\\Fullcalendar\\Facades\\Fullcalendar"
      }
    }
  }
}
